cd /home/pi/git/GPIO_I2C
mkdir bin
javac -d bin /home/pi/git/GPIO_I2C/src/com/rst/gpioi2c/gpio/GPIOMethods.java -h ./src/native/include
javac -d bin /home/pi/git/GPIO_I2C/src/com/rst/gpioi2c/gpio/Press.java -h ./src/native/include
javac -d bin /home/pi/git/GPIO_I2C/src/com/rst/gpioi2c/i2c/I2C_ReadWrite.java -h ./src/native/include
javac -d bin /home/pi/git/GPIO_I2C/src/com/rst/gpioi2c/gpio/WholeGUI.java
javac -d bin /home/pi/git/GPIO_I2C/src/com/rst/gpioi2c/gpio/RguiController.java
javac -d bin /home/pi/git/GPIO_I2C/src/com/rst/gpioi2c/gpio/Rgui.java
javac -d bin /home/pi/git/GPIO_I2C/src/com/rst/gpioi2c/gpio/GPIOPin.java
#javac -d bin /home/pi/git/GPIO_I2C/src/com/rst/gpioi2c/i2c/I2CController.java
#javac -d bin /home/pi/git/GPIO_I2C/src/com/rst/gpioi2c/i2c/GUI_I2C.java
#javac -d bin /home/pi/git/GPIO_I2C/src/com/rst/gpioi2c/All_Of_It.java
#javac -d bin /home/pi/git/GPIO_I2C/src/com/rst/gpioi2c/I2C_AND_GPIO.java
cd /home/pi/git/GPIO_I2C/src/native/c
gcc -o libmyGPIOMethods.so -shared -fPIC -I/usr/lib/jvm/default-java/include -I/usr/lib/jvm/default-java/include/linux GPIOMethods.c -lc 
gcc -o libmyPress.so -shared -fPIC -I/usr/lib/jvm/default-java/include -I/usr/lib/jvm/default-java/include/linux Press.c -lc -lwiringPi 
gcc -o libmyI2C_ReadWrite.so -shared -fPIC -I/usr/lib/jvm/default-java/include -I/usr/lib/jvm/default-java/include/linux I2C_ReadWrite.c -lc 
sudo cp libmyGPIOMethods.so /usr/lib
sudo cp libmyPress.so /usr/lib
sudo cp libmyI2C_ReadWrite.so /usr/lib
cd /home/pi/git/GPIO_I2C
#java -cp bin com.rst.gpioi2c.gpio.GPIOMethods
#java -cp bin com.rst.gpioi2c.gpio.Press
#java -cp bin com.rst.gpioi2c.i2c.I2C_ReadWrite


